{
  "name": "@egvelho/next-material",
  "version": "1.16.0",
  "description": "Boilerplate for Next.js and Material UI",
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "build": "tsc --build tsconfig.build.json",
    "clean": "rm -rf *.d.ts *.js *.js.map components api utils meta netlify-cms storybook-static boilerplate out .next public && mkdir public && touch public/.gitkeep",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "create-boilerplate": "node scripts/create-boilerplate",
    "build-dev": "next build",
    "export-dev": "next export",
    "serve-dev": "serve out -l 3000",
    "generate-assets": "node scripts/generate-assets app.json public",
    "resize-image-assets": "node scripts/resize-image-assets .next/static/images public/images -size 640"
  },
  "bin": {
    "generate-assets": "./scripts/generate-assets.js",
    "resize-image-assets": "./scripts/resize-image-assets.js"
  },
  "author": "egvelho",
  "license": "GPL-3.0",
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "axios": "^0.21.1",
    "dotenv": "^8.2.0",
    "favicons": "^5.5.0",
    "netlify-cms": "^2.0.0",
    "netlify-cms-core": "^2.40.3",
    "next": "^10.0.6",
    "react": "^17.0.1",
    "react-infinite-scroll-component": "^6.0.0",
    "react-masonry-css": "^1.0.14",
    "remark": "^13.0.0",
    "remark-html": "^13.0.1",
    "sharp": "^0.29.0",
    "sitemap": "^6.4.0",
    "slug": "^4.0.3",
    "tslib": "^2.1.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/cli": "^6.2.9",
    "@storybook/react": "^6.2.9",
    "@types/favicons": "^5.5.0",
    "@types/node": "^14.14.31",
    "@types/react": "^17.0.1",
    "@types/sharp": "^0.28.5",
    "@types/slug": "^0.9.1",
    "babel-loader": "^8.2.2",
    "react-dom": "^17.0.1",
    "serve": "^12.0.0",
    "storybook-addon-material-ui": "^0.9.0-alpha.24",
    "typescript": "^4.3.5"
  }
}
