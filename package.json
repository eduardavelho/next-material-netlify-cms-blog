    "sharp": "^0.28.1",
    "sitemap": "^6.4.0",
    "slug": "^4.0.3",
    "tslib": "^2.1.0",
    "typeface-roboto": "0.0.75"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@storybook/addon-actions": "^6.2.9",
