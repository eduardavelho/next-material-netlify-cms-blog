    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "axios": "^0.21.1",
    "babel-plugin-transform-typescript-metadata": "^0.3.1",
    "dotenv": "^8.2.0",
    "favicons": "^5.5.0",
    "material-icons": "^0.3.1",
    "netlify-cms-core": "^2.40.3",
    "next": "^10.0.6",
    "next-images": "^1.7.0",
