{
  "name": "@egvelho/next-material",
  "version": "1.16.0",
  "description": "Boilerplate for Next.js and Material UI",
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "build": "tsc --build tsconfig.build.json",
    "typecheck": "tsc",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "serve-storybook": "serve storybook-static -l 6006",
    "create-boilerplate": "node scripts/create-boilerplate",
    "clean-project": "node scripts/clean-project",
    "build-dev": "next build",
    "export-dev": "next export",
    "serve-dev": "serve out -l 3000",
    "generate-assets": "node ./src/utils/generate-assets app.json public",
    "resize-image-assets": "node ./src/utils/resize-image-assets .next/static/images public/images -size 640"
  },
  "bin": {
    "generate-assets": "./utils/generate-assets.js",
    "resize-image-assets": "./utils/resize-image-assets.js"
  },
  "author": "egvelho",
  "license": "GPL-3.0",
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "axios": "^0.21.1",
    "favicons": "^6.2.2",
    "netlify-cms": "^2.10.158",
    "netlify-cms-core": "^2.47.2",
    "next": "^11.1.0",
    "react": "^17.0.2",
    "react-infinite-scroll-component": "^6.1.0",
    "react-masonry-css": "^1.0.16",
    "remark-highlight.js": "^6.0.0",
    "remark-html": "^13.0.1",
    "remark-parse": "^9.0.0",
    "sharp": "^0.29.0",
    "sitemap": "^7.0.0",
    "slug": "^5.1.0",
    "tslib": "^2.3.1",
    "unified": "^9.2.2"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.3.7",
    "@storybook/addon-essentials": "^6.3.7",
    "@storybook/addon-links": "^6.3.7",
    "@storybook/cli": "^6.3.7",
    "@storybook/react": "^6.3.7",
    "@types/favicons": "^6.2.2",
    "@types/node": "^16.6.2",
    "@types/react": "^17.0.19",
    "@types/sharp": "^0.28.5",
    "@types/slug": "^5.0.2",
    "react-dom": "^17.0.2",
    "serve": "^12.0.0",
    "storybook-addon-material-ui": "^0.9.0-alpha.24",
    "typescript": "^4.3.5"
  }
}
